{
	"name": "sjsitapp",
	"version": "1.0.0",
	"main": "app.js",
	"description": "An application for the San Jose Sharks Ice Team.",
	"repository": "https://github.com/mattcarlotta/SJSITApp",
	"author": "Matt Carlotta",
	"license": "MIT",
	"private": false,
	"engines": {
		"node": ">=8",
		"npm": ">=3"
	},
	"scripts": {
		"dev": "better-npm-run dev",
		"initialize": "yarn install",
		"build": "better-npm-run build",
		"test": "better-npm-run test",
		"test:watch": "better-npm-run test:watch",
		"test:watchall": "better-npm-run test:watchall",
		"test:coverage": "better-npm-run test:coverage",
		"format": "prettier --write 'src/**/*.js'",
		"lintjs": "eslint 'src/**/*.js'",
		"lintstyles": "stylelint **/*.scss",
		"pre-commit": "lint-staged"
	},
	"betterScripts": {
		"dev": {
			"command": "webpack-dev-server",
			"env": {
				"APIPORT": 5000,
				"NODE_ENV": "development",
				"PORT": 3000
			}
		},
		"build": {
			"command": "webpack",
			"env": {
				"APIPORT": 8080,
				"NODE_ENV": "production"
			}
		},
		"test": {
			"command": "jest --config ./jest.json",
			"env": {
				"NODE_ENV": "testing"
			}
		},
		"test:coverage": {
			"command": "jest --config ./jest.json --coverage",
			"env": {
				"NODE_ENV": "testing"
			}
		},
		"test:watch": {
			"command": "jest --config ./jest.json --watch --onlyChanged",
			"env": {
				"NODE_ENV": "testing"
			}
		},
		"test:watchall": {
			"command": "jest --config ./jest.json --watchAll",
			"env": {
				"NODE_ENV": "testing"
			}
		}
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint --fix",
			"git add"
		],
		"*.scss": [
			"stylelint **/*.scss",
			"git add"
		],
		"*.{json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"@nivo/bar": "^0.59.2",
		"@nivo/pie": "^0.59.1",
		"antd": "^3.24.1",
		"axios": "^0.19.0",
		"babel-plugin-import": "^1.12.2",
		"better-npm-run": "^0.1.1",
		"connected-react-router": "^6.5.2",
		"history": "^4.10.1",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"prop-types": "^15.7.2",
		"qs": "^6.9.0",
		"react": "16.8.6",
		"react-beautiful-dnd": "^11.0.5",
		"react-dom": "16.8.6",
		"react-froala-wysiwyg": "^3.0.6",
		"react-helmet": "^5.2.1",
		"react-icons": "^3.7.0",
		"react-quill": "^1.3.3",
		"react-redux": "^7.1.1",
		"react-router-dom": "^5.1.2",
		"react-transition-group": "^4.3.0",
		"redux": "^4.0.1",
		"redux-saga": "^1.1.1",
		"redux-thunk": "^2.3.0",
		"styled-components": "^4.4.0"
	},
	"resolutions": {
		"babel-core": "7.0.0-bridge.0"
	},
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
